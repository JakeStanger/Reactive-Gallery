{
  "name": "reactive-portfolio",
  "version": "0.1.0",
  "private": true,
  "bin": {
    "reactive-gallery-server": "build_server/index.js"
  },
  "scripts": {
    "start:client": "react-scripts start",
    "build:client": "react-scripts build",
    "test:client": "react-scripts test",
    "eject:client": "react-scripts eject",
    "clean:client": "rm -rf build",
    "start:server": "node build_server",
    "build:server": "tsc -p server",
    "clean:server": "rm -rf build_server",
    "prestart": "yarn build:server",
    "start": "concurrently \"yarn start:server\" \"yarn start:client\"",
    "build": "yarn build:server && yarn build:client",
    "clean": "yarn clean:server && yarn clean:client"
  },
  "proxy": "http://localhost:9000",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
